# Copy this to .env and fill in your values

# Required for deployment and on-chain transactions
PRIVATE_KEY=0x_your_wallet_private_key_here

# Optional: defaults to deployer address
FEE_RECIPIENT=0x_fee_recipient_address

# After deployment, set this to use CLI scripts with the contract
ESCROW_ADDRESS=0x_deployed_escrow_address

# Network configuration (defaults provided)
RPC_URL=https://rpc-amoy.polygon.technology
CHAIN_ID=80002
