the deliverable â€” releases USDC to worker minus platform fee\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        uint256 fee = (task.bounty * feeBasisPoints) / 10000;\n        uint256 payout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n            totalFeesCollected += fee;\n        }\n\n        // Transfer payout to worker\n        usdc.safeTransfer(task.worker, payout);\n\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, payout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    // ===================== BIDDING SYSTEM =====================\n\n    /**\n     * @notice Place a bid on an open task\n     * @param taskId Task to bid on\n     * @param price USDC amount the bidder wants for the work\n     * @param estimatedTime Estimated seconds to complete\n     */\n    function bidOnTask(bytes32 taskId, uint256 price, uint256 estimatedTime) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(!hasBid[taskId][msg.sender], \"Already bid on this task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Price exceeds bounty\");\n\n        uint256 bidIndex = task.bidCount;\n        bids[taskId][bidIndex] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n\n        hasBid[taskId][msg.sender] = true;\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime, bidIndex);\n    }\n\n    /**\n     * @notice Accept a bid â€” assigns the task to the bidder at their bid price\n     * @dev Poster already deposited full bounty. Worker receives bid price on approval.\n     *      Any difference between bounty and bid price is refunded to poster immediately.\n     * @param taskId Task with bids\n     * @param bidIndex Index of the bid to accept\n     */\n    function acceptBid(bytes32 taskId, uint256 bidIndex) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bidIndex < task.bidCount, \"Invalid bid index\");\n\n        Bid storage bid = bids[taskId][bidIndex];\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bid.bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bid.bidder, bid.price);\n        emit TaskClaimed(taskId, bid.bidder);\n    }\n\n    /**\n     * @notice Get bid details\n     * @param taskId Task ID\n     * @param bidIndex Bid index\n     */\n    function getBid(bytes32 taskId, uint256 bidIndex) external view returns (Bid memory) {\n        return bids[taskId][bidIndex];\n    }\n\n    // ===================== SUBTASK SYSTEM =====================\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @dev The worker of the parent task becomes the poster of the subtask.\n     *      Subtask bounty comes from the worker (they deposit USDC).\n     * @param parentTaskId Parent task this subtask belongs to\n     * @param subtaskId Unique identifier for the subtask\n     * @param bountyAmount USDC bounty for the subtask\n     */\n    function createSubtask(\n        bytes32 parentTaskId,\n        bytes32 subtaskId,\n        uint256 bountyAmount\n    ) external nonReentrant {\n        Task storage parent = tasks[parentTaskId];\n        require(parent.poster != address(0), \"Parent task does not exist\");\n        require(\n            parent.status == Status.Claimed || parent.status == Status.Submitted,\n            \"Parent task not active\"\n        );\n        require(msg.sender == parent.worker, \"Only parent worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0\n        });\n\n        subtaskIds[parentTaskId].push(subtaskId);\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount);\n    }\n\n    /**\n     * @notice Get subtask IDs for a parent task\n     * @param parentTaskId The parent task ID\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtaskIds[parentTaskId];\n    }\n\n    // ===================== VIEW FUNCTIONS =====================\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Calculate the fee and payout for a given bounty\n     * @param bounty The total bounty amount\n     */\n    function calculateFee(uint256 bounty) external view returns (uint256 fee, uint256 payout) {\n        fee = (bounty * feeBasisPoints) / 10000;\n        payout = bounty - fee;\n    }\n\n    /**\n     * @notice Get platform stats\n     */\n    function getStats() external view returns (\n        uint256 _totalTasks,\n        uint256 _totalCompleted,\n        uint256 _totalVolume,\n        uint256 _totalFees,\n        uint256 _feeBps\n    ) {\n        return (totalTasksCreated, totalTasksCompleted, totalVolumeUsdc, totalFeesCollected, feeBasisPoints);\n    }\n\n    // ===================== ADMIN =====================\n\n    /**\n     * @notice Update platform fee (owner only)\n     * @param newFeeBasisPoints New fee in basis points (e.g., 250 = 2.5%)\n     */\n    function setFee(uint256 newFeeBasisPoints) external onlyOwner {\n        require(newFeeBasisPoints <= MAX_FEE_BASIS_POINTS, \"Fee too high\");\n        uint256 oldFee = feeBasisPoints;\n        feeBasisPoints = newFeeBasisPoints;\n        emit FeeUpdated(oldFee, newFeeBasisPoints);\n    }\n\n    /**\n     * @notice Update fee recipient (owner only)\n     * @param newFeeRecipient New fee recipient address\n     */\n    function setFeeRecipient(address newFeeRecipient) external onlyOwner {\n        require(newFeeRecipient != address(0), \"Invalid address\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = newFeeRecipient;\n        emit FeeRecipientUpdated(oldRecipient, newFeeRecipient);\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_018SgLedVYmyNwrgwq7X3r2z","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice The Agent Economy Protocol â€” USDC escrow for AI agent task marketplace\n * @dev Supports task creation, bidding, subtasks, platform fees, disputes & refunds.\n *      Built for Circle USDC Hackathon on Polygon Amoy.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // --- Platform Fee ---\n    address public feeRecipient;\n    uint256 public feeBasisPoints = 250; // 2.5% = 250 basis points\n    uint256 public constant MAX_FEE_BASIS_POINTS = 1000; // 10% max\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;   // Zero if top-level task\n        uint256 bidCount;\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;          // USDC amount bidder is willing to work for\n        uint256 estimatedTime;  // Estimated seconds to complete\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(uint256 => Bid)) public bids;     // taskId => bidIndex => Bid\n    mapping(bytes32 => mapping(address => bool)) public hasBid;  // taskId => bidder => has bid\n    mapping(bytes32 => bytes32[]) public subtaskIds;             // parentTaskId => child task IDs\n\n    // Platform stats\n    uint256 public totalFeesCollected;\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n\n    // Timeouts\n    uint256 public claimTimeout = 7 days;\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskBid(bytes32 indexed taskId, address indexed bidder, uint256 price, uint256 estimatedTime, uint256 bidIndex);\n    event BidAccepted(bytes32 indexed taskId, address indexed bidder, uint256 price);\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(bytes32 indexed taskId, address indexed worker, uint256 payout, uint256 fee);\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event SubtaskCreated(bytes32 indexed parentTaskId, bytes32 indexed subtaskId, address indexed poster, uint256 bounty);\n    event FeeUpdated(uint256 oldFee, uint256 newFee);\n    event FeeRecipientUpdated(address oldRecipient, address newRecipient);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Only owner\");\n        _;\n    }\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n        owner = msg.sender;\n    }\n\n    // ===================== CORE TASK LIFECYCLE =====================\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (6 decimals)\n     */\n    function createTask(bytes32 taskId, uint256 bountyAmount) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount);\n    }\n\n    /**\n     * @notice Claim an open task directly (no bidding)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable â€” releases USDC to worker minus platform fee\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        uint256 fee = (task.bounty * feeBasisPoints) / 10000;\n        uint256 payout = task.bounty - fee;\n\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n            totalFeesCollected += fee;\n        }\n\n        usdc.safeTransfer(task.worker, payout);\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, payout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    // ===================== BIDDING SYSTEM =====================\n\n    /**\n     * @notice Place a bid on an open task\n     * @param taskId Task to bid on\n     * @param price USDC amount the bidder wants for the work\n     * @param estimatedTime Estimated seconds to complete\n     */\n    function bidOnTask(bytes32 taskId, uint256 price, uint256 estimatedTime) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(!hasBid[taskId][msg.sender], \"Already bid on this task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Price exceeds bounty\");\n\n        uint256 bidIndex = task.bidCount;\n        bids[taskId][bidIndex] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n\n        hasBid[taskId][msg.sender] = true;\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime, bidIndex);\n    }\n\n    /**\n     * @notice Accept a bid â€” assigns the task to the bidder at their bid price\n     * @param taskId Task with bids\n     * @param bidIndex Index of the bid to accept\n     */\n    function acceptBid(bytes32 taskId, uint256 bidIndex) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bidIndex < task.bidCount, \"Invalid bid index\");\n\n        Bid storage bid = bids[taskId][bidIndex];\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bid.bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bid.bidder, bid.price);\n        emit TaskClaimed(taskId, bid.bidder);\n    }\n\n    /**\n     * @notice Get bid details\n     */\n    function getBid(bytes32 taskId, uint256 bidIndex) external view returns (Bid memory) {\n        return bids[taskId][bidIndex];\n    }\n\n    // ===================== SUBTASK SYSTEM =====================\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param parentTaskId Parent task this subtask belongs to\n     * @param subtaskId Unique identifier for the subtask\n     * @param bountyAmount USDC bounty for the subtask\n     */\n    function createSubtask(\n        bytes32 parentTaskId,\n        bytes32 subtaskId,\n        uint256 bountyAmount\n    ) external nonReentrant {\n        Task storage parent = tasks[parentTaskId];\n        require(parent.poster != address(0), \"Parent task does not exist\");\n        require(\n            parent.status == Status.Claimed || parent.status == Status.Submitted,\n            \"Parent task not active\"\n        );\n        require(msg.sender == parent.worker, \"Only parent worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0\n        });\n\n        subtaskIds[parentTaskId].push(subtaskId);\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount);\n    }\n\n    /**\n     * @notice Get subtask IDs for a parent task\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtaskIds[parentTaskId];\n    }\n\n    // ===================== VIEW FUNCTIONS =====================\n\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    function calculateFee(uint256 bounty) external view returns (uint256 fee, uint256 payout) {\n        fee = (bounty * feeBasisPoints) / 10000;\n        payout = bounty - fee;\n    }\n\n    function getStats() external view returns (\n        uint256 _totalTasks,\n        uint256 _totalCompleted,\n        uint256 _totalVolume,\n        uint256 _totalFees,\n        uint256 _feeBps\n    ) {\n        return (totalTasksCreated, totalTasksCompleted, totalVolumeUsdc, totalFeesCollected, feeBasisPoints);\n    }\n\n    // ===================== ADMIN =====================\n\n    function setFee(uint256 newFeeBasisPoints) external onlyOwner {\n        require(newFeeBasisPoints <= MAX_FEE_BASIS_POINTS, \"Fee too high\");\n        uint256 oldFee = feeBasisPoints;\n        feeBasisPoints = newFeeBasisPoints;\n        emit FeeUpdated(oldFee, newFeeBasisPoints);\n    }\n\n    function setFeeRecipient(address newFeeRecipient) external onlyOwner {\n        require(newFeeRecipient != address(0), \"Invalid address\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = newFeeRecipient;\n        emit FeeRecipientUpdated(oldRecipient, newFeeRecipient);\n    }\n}\n"}}],"uuid":"1c4939b5-d2b4-41fe-a828-0a7606c49628"}
