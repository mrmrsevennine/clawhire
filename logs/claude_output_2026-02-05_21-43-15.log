^D{"type":"result","subtype":"success","is_error":false,"duration_ms":349484,"duration_api_ms":353094,"num_turns":12,"result":"I need write permissions to continue. Could you please grant write access to the project directory so I can implement the hackathon project?","session_id":"ef109e71-0258-4ced-895a-81f79ca6d5da","total_cost_usd":1.3771772,"usage":{"input_tokens":14,"cache_creation_input_tokens":40925,"cache_read_input_tokens":632692,"output_tokens":25938,"server_tool_use":{"web_search_requests":0,"web_fetch_requests":0},"service_tier":"standard","cache_creation":{"ephemeral_1h_input_tokens":40925,"ephemeral_5m_input_tokens":0}},"modelUsage":{"claude-opus-4-5-20251101":{"inputTokens":14,"outputTokens":25938,"cacheReadInputTokens":632692,"cacheCreationInputTokens":40925,"webSearchRequests":0,"costUSD":1.22064725,"contextWindow":200000,"maxOutputTokens":64000},"claude-haiku-4-5-20251001":{"inputTokens":6652,"outputTokens":7559,"cacheReadInputTokens":558867,"cacheCreationInputTokens":44957,"webSearchRequests":0,"costUSD":0.15652995,"contextWindow":200000,"maxOutputTokens":64000}},"permission_denials":[{"tool_name":"Write","tool_use_id":"toolu_01YSMNGdnu3wb2sQehLA6bkW","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Escrow contract for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, workers bid and deliver,\n *      poster approves to release funds (minus platform fee).\n *      Supports bidding, subtasks, disputes and refunds.\n */\ncontract TaskEscrow is ReentrancyGuard, Ownable {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% = 250 basis points\n    uint256 public constant FEE_BASIS_POINTS = 250;\n    uint256 public constant BASIS_POINTS_DENOMINATOR = 10000;\n\n    // Fee recipient address\n    address public feeRecipient;\n\n    // Total fees collected (for stats)\n    uint256 public totalFeesCollected;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: parent task ID (0 if root)\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Bid price in USDC (can be <= bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids;  // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;         // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;            // parentTaskId => child task IDs\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // Platform stats\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 agreedPrice\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    constructor(address _usdc, address _feeRecipient) Ownable(msg.sender) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New fee recipient address\n     */\n    function setFeeRecipient(address _newRecipient) external onlyOwner {\n        require(_newRecipient != address(0), \"Invalid fee recipient\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newRecipient);\n    }\n\n    /**\n     * @notice Update timeout periods (owner only)\n     */\n    function setTimeouts(uint256 _claimTimeout, uint256 _disputeWindow) external onlyOwner {\n        require(_claimTimeout >= 1 days, \"Claim timeout too short\");\n        require(_disputeWindow >= 1 days, \"Dispute window too short\");\n        claimTimeout = _claimTimeout;\n        disputeWindow = _disputeWindow;\n    }\n\n    // --- Core Task Functions ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptBids Whether to accept competitive bids (vs direct claim)\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId Parent task this is linked to\n     * @param bountyAmount USDC bounty for the subtask\n     * @param acceptBids Whether to accept bids\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n\n        // Only the worker of the parent task can create subtasks\n        require(\n            msg.sender == parentTask.worker || msg.sender == parentTask.poster,\n            \"Only parent task worker or poster can create subtasks\"\n        );\n\n        // Transfer USDC from creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptBids, parentTaskId);\n    }\n\n    // --- Bidding Functions ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Your bid price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid cannot exceed bounty\");\n        require(estimatedTime > 0, \"Estimated time must be > 0\");\n        require(bids[taskId][msg.sender].bidder == address(0), \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid on your task\n     * @param taskId Task ID\n     * @param bidder Address of the bidder to accept\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bid\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n\n        Bid storage bid = bids[taskId][bidder];\n        require(bid.bidder != address(0), \"Bid does not exist\");\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;  // Update bounty to accepted price\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, bid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    /**\n     * @notice Claim an open task directly (non-bidding tasks only)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding, use bidOnTask\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n            totalFeesCollected += fee;\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task before anyone claims/bids\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n        require(task.bidCount == 0, \"Cannot cancel task with bids\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get all bids for a task\n     * @param taskId Task to query\n     */\n    function getTaskBids(bytes32 taskId) external view returns (Bid[] memory) {\n        address[] memory bidderList = taskBidders[taskId];\n        Bid[] memory bidList = new Bid[](bidderList.length);\n\n        for (uint256 i = 0; i < bidderList.length; i++) {\n            bidList[i] = bids[taskId][bidderList[i]];\n        }\n\n        return bidList;\n    }\n\n    /**\n     * @notice Get subtasks of a parent task\n     * @param parentTaskId Parent task ID\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Get platform statistics\n     */\n    function getPlatformStats() external view returns (\n        uint256 tasksCreated,\n        uint256 tasksCompleted,\n        uint256 volumeUsdc,\n        uint256 feesCollected\n    ) {\n        return (\n            totalTasksCreated,\n            totalTasksCompleted,\n            totalVolumeUsdc,\n            totalFeesCollected\n        );\n    }\n\n    /**\n     * @notice Calculate the fee for a given bounty amount\n     * @param bountyAmount Bounty to calculate fee for\n     */\n    function calculateFee(uint256 bountyAmount) external pure returns (uint256 fee, uint256 workerPayout) {\n        fee = (bountyAmount * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        workerPayout = bountyAmount - fee;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_018gsAGR2nYED1Do1noHS38v","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Escrow contract for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, workers bid and deliver,\n *      poster approves to release funds (minus platform fee).\n *      Supports bidding, subtasks, disputes and refunds.\n */\ncontract TaskEscrow is ReentrancyGuard, Ownable {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% = 250 basis points\n    uint256 public constant FEE_BASIS_POINTS = 250;\n    uint256 public constant BASIS_POINTS_DENOMINATOR = 10000;\n\n    // Fee recipient address\n    address public feeRecipient;\n\n    // Total fees collected (for stats)\n    uint256 public totalFeesCollected;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: parent task ID (0 if root)\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Bid price in USDC (can be <= bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids;  // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;         // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;            // parentTaskId => child task IDs\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // Platform stats\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 agreedPrice\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    constructor(address _usdc, address _feeRecipient) Ownable(msg.sender) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New fee recipient address\n     */\n    function setFeeRecipient(address _newRecipient) external onlyOwner {\n        require(_newRecipient != address(0), \"Invalid fee recipient\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newRecipient);\n    }\n\n    /**\n     * @notice Update timeout periods (owner only)\n     */\n    function setTimeouts(uint256 _claimTimeout, uint256 _disputeWindow) external onlyOwner {\n        require(_claimTimeout >= 1 days, \"Claim timeout too short\");\n        require(_disputeWindow >= 1 days, \"Dispute window too short\");\n        claimTimeout = _claimTimeout;\n        disputeWindow = _disputeWindow;\n    }\n\n    // --- Core Task Functions ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptBids Whether to accept competitive bids (vs direct claim)\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId Parent task this is linked to\n     * @param bountyAmount USDC bounty for the subtask\n     * @param acceptBids Whether to accept bids\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n\n        // Only the worker of the parent task can create subtasks\n        require(\n            msg.sender == parentTask.worker || msg.sender == parentTask.poster,\n            \"Only parent task worker or poster can create subtasks\"\n        );\n\n        // Transfer USDC from creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptBids, parentTaskId);\n    }\n\n    // --- Bidding Functions ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Your bid price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid cannot exceed bounty\");\n        require(estimatedTime > 0, \"Estimated time must be > 0\");\n        require(bids[taskId][msg.sender].bidder == address(0), \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid on your task\n     * @param taskId Task ID\n     * @param bidder Address of the bidder to accept\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bid\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n\n        Bid storage bid = bids[taskId][bidder];\n        require(bid.bidder != address(0), \"Bid does not exist\");\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;  // Update bounty to accepted price\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, bid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    /**\n     * @notice Claim an open task directly (non-bidding tasks only)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding, use bidOnTask\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n            totalFeesCollected += fee;\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task before anyone claims/bids\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n        require(task.bidCount == 0, \"Cannot cancel task with bids\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get all bids for a task\n     * @param taskId Task to query\n     */\n    function getTaskBids(bytes32 taskId) external view returns (Bid[] memory) {\n        address[] memory bidderList = taskBidders[taskId];\n        Bid[] memory bidList = new Bid[](bidderList.length);\n\n        for (uint256 i = 0; i < bidderList.length; i++) {\n            bidList[i] = bids[taskId][bidderList[i]];\n        }\n\n        return bidList;\n    }\n\n    /**\n     * @notice Get subtasks of a parent task\n     * @param parentTaskId Parent task ID\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Get platform statistics\n     */\n    function getPlatformStats() external view returns (\n        uint256 tasksCreated,\n        uint256 tasksCompleted,\n        uint256 volumeUsdc,\n        uint256 feesCollected\n    ) {\n        return (\n            totalTasksCreated,\n            totalTasksCompleted,\n            totalVolumeUsdc,\n            totalFeesCollected\n        );\n    }\n\n    /**\n     * @notice Calculate the fee for a given bounty amount\n     * @param bountyAmount Bounty to calculate fee for\n     */\n    function calculateFee(uint256 bountyAmount) external pure returns (uint256 fee, uint256 workerPayout) {\n        fee = (bountyAmount * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        workerPayout = bountyAmount - fee;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_019ETfZvfKShSL4x1bibBaKj","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Escrow contract for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, workers bid and deliver,\n *      poster approves to release funds (minus platform fee).\n *      Supports bidding, subtasks, disputes and refunds.\n */\ncontract TaskEscrow is ReentrancyGuard, Ownable {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% = 250 basis points\n    uint256 public constant FEE_BASIS_POINTS = 250;\n    uint256 public constant BASIS_POINTS_DENOMINATOR = 10000;\n\n    // Fee recipient address\n    address public feeRecipient;\n\n    // Total fees collected (for stats)\n    uint256 public totalFeesCollected;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: parent task ID (0 if root)\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Bid price in USDC (can be <= bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids;  // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;         // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;            // parentTaskId => child task IDs\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // Platform stats\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 agreedPrice\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    constructor(address _usdc, address _feeRecipient) Ownable(msg.sender) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New fee recipient address\n     */\n    function setFeeRecipient(address _newRecipient) external onlyOwner {\n        require(_newRecipient != address(0), \"Invalid fee recipient\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newRecipient);\n    }\n\n    /**\n     * @notice Update timeout periods (owner only)\n     */\n    function setTimeouts(uint256 _claimTimeout, uint256 _disputeWindow) external onlyOwner {\n        require(_claimTimeout >= 1 days, \"Claim timeout too short\");\n        require(_disputeWindow >= 1 days, \"Dispute window too short\");\n        claimTimeout = _claimTimeout;\n        disputeWindow = _disputeWindow;\n    }\n\n    // --- Core Task Functions ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptBids Whether to accept competitive bids (vs direct claim)\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId Parent task this is linked to\n     * @param bountyAmount USDC bounty for the subtask\n     * @param acceptBids Whether to accept bids\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n\n        // Only the worker of the parent task can create subtasks\n        require(\n            msg.sender == parentTask.worker || msg.sender == parentTask.poster,\n            \"Only parent task worker or poster can create subtasks\"\n        );\n\n        // Transfer USDC from creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptBids, parentTaskId);\n    }\n\n    // --- Bidding Functions ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Your bid price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid cannot exceed bounty\");\n        require(estimatedTime > 0, \"Estimated time must be > 0\");\n        require(bids[taskId][msg.sender].bidder == address(0), \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid on your task\n     * @param taskId Task ID\n     * @param bidder Address of the bidder to accept\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bid\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n\n        Bid storage bid = bids[taskId][bidder];\n        require(bid.bidder != address(0), \"Bid does not exist\");\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;  // Update bounty to accepted price\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, bid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    /**\n     * @notice Claim an open task directly (non-bidding tasks only)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding, use bidOnTask\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n            totalFeesCollected += fee;\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task before anyone claims/bids\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n        require(task.bidCount == 0, \"Cannot cancel task with bids\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get all bids for a task\n     * @param taskId Task to query\n     */\n    function getTaskBids(bytes32 taskId) external view returns (Bid[] memory) {\n        address[] memory bidderList = taskBidders[taskId];\n        Bid[] memory bidList = new Bid[](bidderList.length);\n\n        for (uint256 i = 0; i < bidderList.length; i++) {\n            bidList[i] = bids[taskId][bidderList[i]];\n        }\n\n        return bidList;\n    }\n\n    /**\n     * @notice Get subtasks of a parent task\n     * @param parentTaskId Parent task ID\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Get platform statistics\n     */\n    function getPlatformStats() external view returns (\n        uint256 tasksCreated,\n        uint256 tasksCompleted,\n        uint256 volumeUsdc,\n        uint256 feesCollected\n    ) {\n        return (\n            totalTasksCreated,\n            totalTasksCompleted,\n            totalVolumeUsdc,\n            totalFeesCollected\n        );\n    }\n\n    /**\n     * @notice Calculate the fee for a given bounty amount\n     * @param bountyAmount Bounty to calculate fee for\n     */\n    function calculateFee(uint256 bountyAmount) external pure returns (uint256 fee, uint256 workerPayout) {\n        fee = (bountyAmount * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        workerPayout = bountyAmount - fee;\n    }\n}\n"}},{"tool_name":"AskUserQuestion","tool_use_id":"toolu_01MWpwVMBsWGfDiL7w5W2Y9h","tool_input":{"questions":[{"question":"I need permission to write files to build the hackathon project. Can you grant write access to the skill/usdc-agent-tasks directory?","header":"Permissions","options":[{"label":"Grant access","description":"Allow writing to the claw-marketplace directory to build the project"},{"label":"Deny access","description":"I'll explain what changes need to be made instead"}],"multiSelect":false}]}},{"tool_name":"Write","tool_use_id":"toolu_01NY97VWut538WtFgu1yGRCj","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Escrow contract for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, workers bid and deliver,\n *      poster approves to release funds (minus platform fee).\n *      Supports bidding, subtasks, disputes and refunds.\n */\ncontract TaskEscrow is ReentrancyGuard, Ownable {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% = 250 basis points\n    uint256 public constant FEE_BASIS_POINTS = 250;\n    uint256 public constant BASIS_POINTS_DENOMINATOR = 10000;\n\n    // Fee recipient address\n    address public feeRecipient;\n\n    // Total fees collected (for stats)\n    uint256 public totalFeesCollected;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: parent task ID (0 if root)\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Bid price in USDC (can be <= bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids;  // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;         // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;            // parentTaskId => child task IDs\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // Platform stats\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 agreedPrice\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    constructor(address _usdc, address _feeRecipient) Ownable(msg.sender) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New fee recipient address\n     */\n    function setFeeRecipient(address _newRecipient) external onlyOwner {\n        require(_newRecipient != address(0), \"Invalid fee recipient\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newRecipient);\n    }\n\n    /**\n     * @notice Update timeout periods (owner only)\n     */\n    function setTimeouts(uint256 _claimTimeout, uint256 _disputeWindow) external onlyOwner {\n        require(_claimTimeout >= 1 days, \"Claim timeout too short\");\n        require(_disputeWindow >= 1 days, \"Dispute window too short\");\n        claimTimeout = _claimTimeout;\n        disputeWindow = _disputeWindow;\n    }\n\n    // --- Core Task Functions ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptBids Whether to accept competitive bids (vs direct claim)\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId Parent task this is linked to\n     * @param bountyAmount USDC bounty for the subtask\n     * @param acceptBids Whether to accept bids\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n\n        // Only the worker of the parent task can create subtasks\n        require(\n            msg.sender == parentTask.worker || msg.sender == parentTask.poster,\n            \"Only parent task worker or poster can create subtasks\"\n        );\n\n        // Transfer USDC from creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptBids, parentTaskId);\n    }\n\n    // --- Bidding Functions ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Your bid price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid cannot exceed bounty\");\n        require(estimatedTime > 0, \"Estimated time must be > 0\");\n        require(bids[taskId][msg.sender].bidder == address(0), \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid on your task\n     * @param taskId Task ID\n     * @param bidder Address of the bidder to accept\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bid\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n\n        Bid storage bid = bids[taskId][bidder];\n        require(bid.bidder != address(0), \"Bid does not exist\");\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;  // Update bounty to accepted price\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, bid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    /**\n     * @notice Claim an open task directly (non-bidding tasks only)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding, use bidOnTask\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n            totalFeesCollected += fee;\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task before anyone claims/bids\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n        require(task.bidCount == 0, \"Cannot cancel task with bids\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get all bids for a task\n     * @param taskId Task to query\n     */\n    function getTaskBids(bytes32 taskId) external view returns (Bid[] memory) {\n        address[] memory bidderList = taskBidders[taskId];\n        Bid[] memory bidList = new Bid[](bidderList.length);\n\n        for (uint256 i = 0; i < bidderList.length; i++) {\n            bidList[i] = bids[taskId][bidderList[i]];\n        }\n\n        return bidList;\n    }\n\n    /**\n     * @notice Get subtasks of a parent task\n     * @param parentTaskId Parent task ID\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Get platform statistics\n     */\n    function getPlatformStats() external view returns (\n        uint256 tasksCreated,\n        uint256 tasksCompleted,\n        uint256 volumeUsdc,\n        uint256 feesCollected\n    ) {\n        return (\n            totalTasksCreated,\n            totalTasksCompleted,\n            totalVolumeUsdc,\n            totalFeesCollected\n        );\n    }\n\n    /**\n     * @notice Calculate the fee for a given bounty amount\n     * @param bountyAmount Bounty to calculate fee for\n     */\n    function calculateFee(uint256 bountyAmount) external pure returns (uint256 fee, uint256 workerPayout) {\n        fee = (bountyAmount * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        workerPayout = bountyAmount - fee;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_01ETLbLAhXVT4Kmvt5dbvAZ4","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Escrow contract for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, workers bid and deliver,\n *      poster approves to release funds (minus platform fee).\n *      Supports bidding, subtasks, disputes and refunds.\n */\ncontract TaskEscrow is ReentrancyGuard, Ownable {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% = 250 basis points\n    uint256 public constant FEE_BASIS_POINTS = 250;\n    uint256 public constant BASIS_POINTS_DENOMINATOR = 10000;\n\n    // Fee recipient address\n    address public feeRecipient;\n\n    // Total fees collected (for stats)\n    uint256 public totalFeesCollected;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: parent task ID (0 if root)\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Bid price in USDC (can be <= bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids;  // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;         // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;            // parentTaskId => child task IDs\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // Platform stats\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 agreedPrice\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    constructor(address _usdc, address _feeRecipient) Ownable(msg.sender) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New fee recipient address\n     */\n    function setFeeRecipient(address _newRecipient) external onlyOwner {\n        require(_newRecipient != address(0), \"Invalid fee recipient\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newRecipient);\n    }\n\n    /**\n     * @notice Update timeout periods (owner only)\n     */\n    function setTimeouts(uint256 _claimTimeout, uint256 _disputeWindow) external onlyOwner {\n        require(_claimTimeout >= 1 days, \"Claim timeout too short\");\n        require(_disputeWindow >= 1 days, \"Dispute window too short\");\n        claimTimeout = _claimTimeout;\n        disputeWindow = _disputeWindow;\n    }\n\n    // --- Core Task Functions ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptBids Whether to accept competitive bids (vs direct claim)\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId Parent task this is linked to\n     * @param bountyAmount USDC bounty for the subtask\n     * @param acceptBids Whether to accept bids\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount,\n        bool acceptBids\n    ) external nonReentrant {\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n\n        // Only the worker of the parent task can create subtasks\n        require(\n            msg.sender == parentTask.worker || msg.sender == parentTask.poster,\n            \"Only parent task worker or poster can create subtasks\"\n        );\n\n        // Transfer USDC from creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptBids, parentTaskId);\n    }\n\n    // --- Bidding Functions ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Your bid price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid cannot exceed bounty\");\n        require(estimatedTime > 0, \"Estimated time must be > 0\");\n        require(bids[taskId][msg.sender].bidder == address(0), \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid on your task\n     * @param taskId Task ID\n     * @param bidder Address of the bidder to accept\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bid\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n\n        Bid storage bid = bids[taskId][bidder];\n        require(bid.bidder != address(0), \"Bid does not exist\");\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;  // Update bounty to accepted price\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, bid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    /**\n     * @notice Claim an open task directly (non-bidding tasks only)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding, use bidOnTask\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n            totalFeesCollected += fee;\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task before anyone claims/bids\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n        require(task.bidCount == 0, \"Cannot cancel task with bids\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get all bids for a task\n     * @param taskId Task to query\n     */\n    function getTaskBids(bytes32 taskId) external view returns (Bid[] memory) {\n        address[] memory bidderList = taskBidders[taskId];\n        Bid[] memory bidList = new Bid[](bidderList.length);\n\n        for (uint256 i = 0; i < bidderList.length; i++) {\n            bidList[i] = bids[taskId][bidderList[i]];\n        }\n\n        return bidList;\n    }\n\n    /**\n     * @notice Get subtasks of a parent task\n     * @param parentTaskId Parent task ID\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Get platform statistics\n     */\n    function getPlatformStats() external view returns (\n        uint256 tasksCreated,\n        uint256 tasksCompleted,\n        uint256 volumeUsdc,\n        uint256 feesCollected\n    ) {\n        return (\n            totalTasksCreated,\n            totalTasksCompleted,\n            totalVolumeUsdc,\n            totalFeesCollected\n        );\n    }\n\n    /**\n     * @notice Calculate the fee for a given bounty amount\n     * @param bountyAmount Bounty to calculate fee for\n     */\n    function calculateFee(uint256 bountyAmount) external pure returns (uint256 fee, uint256 workerPayout) {\n        fee = (bountyAmount * FEE_BASIS_POINTS) / BASIS_POINTS_DENOMINATOR;\n        workerPayout = bountyAmount - fee;\n    }\n}\n"}}],"uuid":"9655fac0-8783-4b56-a3bf-8f6ca4c7e970"}
