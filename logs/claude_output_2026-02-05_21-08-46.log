  // Fee recipient address (platform treasury)\n    address public feeRecipient;\n\n    // Contract owner for admin functions\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: links to parent task\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // True if task is in bidding mode\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Bid price in USDC (can be <= bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n        bool exists;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n\n    // taskId => bidder => Bid\n    mapping(bytes32 => mapping(address => Bid)) public bids;\n\n    // taskId => array of bidder addresses (for enumeration)\n    mapping(bytes32 => address[]) public taskBidders;\n\n    // parentTaskId => array of subtask IDs\n    mapping(bytes32 => bytes32[]) public subtasks;\n\n    // Platform statistics\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n    uint256 public totalFeesCollected;\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n    event OwnershipTransferred(address indexed oldOwner, address indexed newOwner);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Only owner\");\n        _;\n    }\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n        owner = msg.sender;\n    }\n\n    // ============ Admin Functions ============\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New fee recipient address\n     */\n    function setFeeRecipient(address _newRecipient) external onlyOwner {\n        require(_newRecipient != address(0), \"Invalid address\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newRecipient);\n    }\n\n    /**\n     * @notice Transfer contract ownership\n     * @param _newOwner New owner address\n     */\n    function transferOwnership(address _newOwner) external onlyOwner {\n        require(_newOwner != address(0), \"Invalid address\");\n        address oldOwner = owner;\n        owner = _newOwner;\n        emit OwnershipTransferred(oldOwner, _newOwner);\n    }\n\n    // ============ Task Creation ============\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptingBids If true, task enters bidding mode; if false, first-claim mode\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptingBids);\n    }\n\n    /**\n     * @notice Create a task with default first-claim mode (backward compatible)\n     */\n    function createTask(bytes32 taskId, uint256 bountyAmount) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: false\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, false);\n    }\n\n    // ============ Subtask Creation ============\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @dev Only the worker of the parent task can create subtasks\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param subtaskId Unique identifier for the subtask\n     * @param bountyAmount USDC bounty for the subtask\n     * @param acceptingBids If true, subtask enters bidding mode\n     */\n    function createSubtask(\n        bytes32 parentTaskId,\n        bytes32 subtaskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n        require(\n            parentTask.status == Status.Claimed || parentTask.status == Status.Open,\n            \"Parent task not active\"\n        );\n        // Worker of parent task OR poster can create subtasks\n        require(\n            msg.sender == parentTask.worker || msg.sender == parentTask.poster,\n            \"Not authorized to create subtask\"\n        );\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from caller to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptingBids);\n    }\n\n    // ============ Bidding System ============\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Bid price in USDC (must be <= task bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid exceeds bounty\");\n        require(!bids[taskId][msg.sender].exists, \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false,\n            exists: true\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to the bidder\n     * @param taskId Task to accept bid for\n     * @param bidder Address of the bidder to accept\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bids[taskId][bidder].exists, \"Bid does not exist\");\n        require(!bids[taskId][bidder].accepted, \"Bid already accepted\");\n\n        Bid storage bid = bids[taskId][bidder];\n        bid.accepted = true;\n\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price; // Update bounty to accepted bid price\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, bid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    // ============ Task Claiming (First-Come Mode) ============\n\n    /**\n     * @notice Claim an open task as a worker (first-come-first-served mode)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task is in bidding mode, use bidOnTask\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    // ============ Work Submission ============\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    // ============ Task Approval & Payment ============\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 platformFee = (task.bounty * PLATFORM_FEE_BPS) / BPS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - platformFee;\n\n        // Transfer fee to platform\n        if (platformFee > 0) {\n            usdc.safeTransfer(feeRecipient, platformFee);\n            totalFeesCollected += platformFee;\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, workerPayout, platformFee);\n    }\n\n    // ============ Disputes & Refunds ============\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task that has no bids or claims\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.worker == address(0), \"Task already has worker\");\n        require(task.bidCount == 0, \"Task has bids, cannot cancel\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster);\n    }\n\n    // ============ View Functions ============\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get bid details\n     * @param taskId Task ID\n     * @param bidder Bidder address\n     */\n    function getBid(bytes32 taskId, address bidder) external view returns (Bid memory) {\n        return bids[taskId][bidder];\n    }\n\n    /**\n     * @notice Get all bidders for a task\n     * @param taskId Task ID\n     */\n    function getTaskBidders(bytes32 taskId) external view returns (address[] memory) {\n        return taskBidders[taskId];\n    }\n\n    /**\n     * @notice Get all subtasks of a parent task\n     * @param parentTaskId Parent task ID\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Get platform statistics\n     */\n    function getStats() external view returns (\n        uint256 tasksCreated,\n        uint256 tasksCompleted,\n        uint256 volumeUsdc,\n        uint256 feesCollected\n    ) {\n        return (totalTasksCreated, totalTasksCompleted, totalVolumeUsdc, totalFeesCollected);\n    }\n\n    /**\n     * @notice Calculate the platform fee for a given bounty amount\n     * @param bountyAmount The bounty amount to calculate fee for\n     */\n    function calculateFee(uint256 bountyAmount) external pure returns (uint256 fee, uint256 workerPayout) {\n        fee = (bountyAmount * PLATFORM_FEE_BPS) / BPS_DENOMINATOR;\n        workerPayout = bountyAmount - fee;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_01192WQNypHwTxAfxzCY9hSR","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Escrow contract for USDC-based agent task marketplace\n * @dev The Agent Economy Protocol: Agents post tasks with USDC bounties,\n *      other agents bid on work, poster accepts best bid, worker delivers,\n *      and gets paid automatically with 2.5% platform fee.\n *      Supports agent-to-agent subcontracting via subtasks.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% (250 basis points out of 10000)\n    uint256 public constant PLATFORM_FEE_BPS = 250;\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    // Fee recipient address (platform treasury)\n    address public feeRecipient;\n\n    // Contract owner for admin functions\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: links to parent task\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // True if task is in bidding mode\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Bid price in USDC (can be <= bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n        bool exists;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n\n    // taskId => bidder => Bid\n    mapping(bytes32 => mapping(address => Bid)) public bids;\n\n    // taskId => array of bidder addresses (for enumeration)\n    mapping(bytes32 => address[]) public taskBidders;\n\n    // parentTaskId => array of subtask IDs\n    mapping(bytes32 => bytes32[]) public subtasks;\n\n    // Platform statistics\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n    uint256 public totalFeesCollected;\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n    event OwnershipTransferred(address indexed oldOwner, address indexed newOwner);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Only owner\");\n        _;\n    }\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n        owner = msg.sender;\n    }\n\n    // ============ Admin Functions ============\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New fee recipient address\n     */\n    function setFeeRecipient(address _newRecipient) external onlyOwner {\n        require(_newRecipient != address(0), \"Invalid address\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newRecipient);\n    }\n\n    /**\n     * @notice Transfer contract ownership\n     * @param _newOwner New owner address\n     */\n    function transferOwnership(address _newOwner) external onlyOwner {\n        require(_newOwner != address(0), \"Invalid address\");\n        address oldOwner = owner;\n        owner = _newOwner;\n        emit OwnershipTransferred(oldOwner, _newOwner);\n    }\n\n    // ============ Task Creation ============\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptingBids If true, task enters bidding mode; if false, first-claim mode\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptingBids);\n    }\n\n    /**\n     * @notice Create a task with default first-claim mode (backward compatible)\n     */\n    function createTaskSimple(bytes32 taskId, uint256 bountyAmount) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: false\n        });\n\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, false);\n    }\n\n    // ============ Subtask Creation ============\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @dev Only the worker of the parent task can create subtasks\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param subtaskId Unique identifier for the subtask\n     * @param bountyAmount USDC bounty for the subtask\n     * @param acceptingBids If true, subtask enters bidding mode\n     */\n    function createSubtask(\n        bytes32 parentTaskId,\n        bytes32 subtaskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n        require(\n            parentTask.status == Status.Claimed || parentTask.status == Status.Open,\n            \"Parent task not active\"\n        );\n        // Worker of parent task OR poster can create subtasks\n        require(\n            msg.sender == parentTask.worker || msg.sender == parentTask.poster,\n            \"Not authorized to create subtask\"\n        );\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from caller to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n        totalVolumeUsdc += bountyAmount;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptingBids);\n    }\n\n    // ============ Bidding System ============\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Bid price in USDC (must be <= task bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid exceeds bounty\");\n        require(!bids[taskId][msg.sender].exists, \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false,\n            exists: true\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to the bidder\n     * @param taskId Task to accept bid for\n     * @param bidder Address of the bidder to accept\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bids[taskId][bidder].exists, \"Bid does not exist\");\n        require(!bids[taskId][bidder].accepted, \"Bid already accepted\");\n\n        Bid storage bid = bids[taskId][bidder];\n        bid.accepted = true;\n\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price; // Update bounty to accepted bid price\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, bid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    // ============ Task Claiming (First-Come Mode) ============\n\n    /**\n     * @notice Claim an open task as a worker (first-come-first-served mode)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task is in bidding mode, use bidOnTask\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    // ============ Work Submission ============\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    // ============ Task Approval & Payment ============\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 platformFee = (task.bounty * PLATFORM_FEE_BPS) / BPS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - platformFee;\n\n        // Transfer fee to platform\n        if (platformFee > 0) {\n            usdc.safeTransfer(feeRecipient, platformFee);\n            totalFeesCollected += platformFee;\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        totalTasksCompleted++;\n\n        emit TaskApproved(taskId, task.worker, workerPayout, platformFee);\n    }\n\n    // ============ Disputes & Refunds ============\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task that has no bids or claims\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.worker == address(0), \"Task already has worker\");\n        require(task.bidCount == 0, \"Task has bids, cannot cancel\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster);\n    }\n\n    // ============ View Functions ============\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get bid details\n     * @param taskId Task ID\n     * @param bidder Bidder address\n     */\n    function getBid(bytes32 taskId, address bidder) external view returns (Bid memory) {\n        return bids[taskId][bidder];\n    }\n\n    /**\n     * @notice Get all bidders for a task\n     * @param taskId Task ID\n     */\n    function getTaskBidders(bytes32 taskId) external view returns (address[] memory) {\n        return taskBidders[taskId];\n    }\n\n    /**\n     * @notice Get all subtasks of a parent task\n     * @param parentTaskId Parent task ID\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Get platform statistics\n     */\n    function getStats() external view returns (\n        uint256 tasksCreated,\n        uint256 tasksCompleted,\n        uint256 volumeUsdc,\n        uint256 feesCollected\n    ) {\n        return (totalTasksCreated, totalTasksCompleted, totalVolumeUsdc, totalFeesCollected);\n    }\n\n    /**\n     * @notice Calculate the platform fee for a given bounty amount\n     * @param bountyAmount The bounty amount to calculate fee for\n     */\n    function calculateFee(uint256 bountyAmount) external pure returns (uint256 fee, uint256 workerPayout) {\n        fee = (bountyAmount * PLATFORM_FEE_BPS) / BPS_DENOMINATOR;\n        workerPayout = bountyAmount - fee;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_01B8fHaXjQypSnB5dfDRmuZs","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Escrow contract for USDC-based agent task marketplace\n * @dev The Agent Economy Protocol: Agents post tasks with USDC bounties,\n *      other agents bid on work, poster accepts best bid, worker delivers,\n *      and gets paid automatically with 2.5% platform fee.\n *      Supports agent-to-agent subcontracting via subtasks.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% (250 basis points out of 10000)\n    uint256 public constant PLATFORM_FEE_BPS = 250;\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    // Fee recipient address (platform treasury)\n    address public feeRecipient;\n\n    // Contract owner for admin functions\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: links to parent task\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // True if task is in bidding mode\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Bid price in USDC (can be <= bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n        bool exists;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n\n    // taskId => bidder => Bid\n    mapping(bytes32 => mapping(address => Bid)) public bids;\n\n    // taskId => array of bidder addresses (for enumeration)\n    mapping(bytes32 => address[]) public taskBidders;\n\n    // parentTaskId => array of subtask IDs\n    mapping(bytes32 => bytes32[]) public subtasks;\n\n    // Platform statistics\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalVolumeUsdc;\n    uint256 public totalFeesCollected;\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n    event OwnershipTransferred(address indexed oldOwner, address indexed newOwner);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Only owner\");\n        _;\n    }\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n        owner = msg.sender;\n    }\n\n    // ============ Admin Functions ============\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New fee recipient address\n     */\n    function setFeeRecipient(address _newRecipient) external