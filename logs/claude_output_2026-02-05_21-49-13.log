ity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice The Agent Economy Protocol - Escrow for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, other agents bid on work,\n *      poster accepts best bid, worker delivers, payment flows automatically.\n *      Supports agent-to-agent subcontracting and platform fees.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee configuration\n    address public feeRecipient;\n    uint256 public platformFeeBps = 250; // 2.5% = 250 basis points\n    uint256 public constant MAX_FEE_BPS = 1000; // Max 10%\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: links to parent task\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids vs direct claim\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Proposed price (can be lower than bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n        bool exists;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids; // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;        // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;           // parentTaskId => child taskIds\n\n    // Timeout: poster can refund if no claim/bid accepted after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster, uint256 refundAmount);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n    event PlatformFeeUpdated(uint256 oldFeeBps, uint256 newFeeBps);\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New address to receive platform fees\n     */\n    function setFeeRecipient(address _newRecipient) external {\n        require(msg.sender == feeRecipient, \"Only fee recipient can update\");\n        require(_newRecipient != address(0), \"Invalid address\");\n        emit FeeRecipientUpdated(feeRecipient, _newRecipient);\n        feeRecipient = _newRecipient;\n    }\n\n    /**\n     * @notice Update the platform fee (restricted to fee recipient)\n     * @param _newFeeBps New fee in basis points (100 = 1%)\n     */\n    function setPlatformFee(uint256 _newFeeBps) external {\n        require(msg.sender == feeRecipient, \"Only fee recipient can update\");\n        require(_newFeeBps <= MAX_FEE_BPS, \"Fee too high\");\n        emit PlatformFeeUpdated(platformFeeBps, _newFeeBps);\n        platformFeeBps = _newFeeBps;\n    }\n\n    // --- Task Creation ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptingBids If true, task accepts bids; if false, direct claim allowed\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptingBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @dev Only the worker of the parent task can create subtasks\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param subtaskId Unique identifier for the subtask\n     * @param bountyAmount USDC amount for the subtask\n     * @param acceptingBids If true, subtask accepts bids\n     */\n    function createSubtask(\n        bytes32 parentTaskId,\n        bytes32 subtaskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n        require(\n            parentTask.status == Status.Claimed || parentTask.status == Status.Submitted,\n            \"Parent task must be active\"\n        );\n        require(msg.sender == parentTask.worker, \"Only parent task worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from subtask creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptingBids, parentTaskId);\n    }\n\n    // --- Bidding System ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Proposed price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Price cannot exceed bounty\");\n        require(!bids[taskId][msg.sender].exists, \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false,\n            exists: true\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to the bidder\n     * @param taskId Task to assign\n     * @param bidder Address of the winning bidder\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bids[taskId][bidder].exists, \"Bid does not exist\");\n\n        Bid storage winningBid = bids[taskId][bidder];\n        winningBid.accepted = true;\n\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (winningBid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - winningBid.price;\n            task.bounty = winningBid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, winningBid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    // --- Direct Claim (for non-bidding tasks) ---\n\n    /**\n     * @notice Claim an open task as a worker (only for non-bidding tasks)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    // --- Work Submission & Approval ---\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee\n        uint256 fee = (task.bounty * platformFeeBps) / BPS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    // --- Disputes & Refunds ---\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task and get full refund (only before any claims/bids accepted)\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster, task.bounty);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get a bid for a specific task and bidder\n     * @param taskId Task to query\n     * @param bidder Bidder address\n     */\n    function getBid(bytes32 taskId, address bidder) external view returns (Bid memory) {\n        return bids[taskId][bidder];\n    }\n\n    /**\n     * @notice Get all bidders for a task\n     * @param taskId Task to query\n     */\n    function getTaskBidders(bytes32 taskId) external view returns (address[] memory) {\n        return taskBidders[taskId];\n    }\n\n    /**\n     * @notice Get all subtask IDs for a parent task\n     * @param parentTaskId Parent task to query\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Calculate the platform fee and worker payout for a given bounty\n     * @param bounty The task bounty amount\n     */\n    function calculateFee(uint256 bounty) external view returns (uint256 fee, uint256 workerPayout) {\n        fee = (bounty * platformFeeBps) / BPS_DENOMINATOR;\n        workerPayout = bounty - fee;\n    }\n\n    /**\n     * @notice Get the current platform fee in basis points\n     */\n    function getPlatformFeeBps() external view returns (uint256) {\n        return platformFeeBps;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_01TYLDupwTqLMhYBuZzpsJ2r","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice The Agent Economy Protocol - Escrow for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, other agents bid on work,\n *      poster accepts best bid, worker delivers, payment flows automatically.\n *      Supports agent-to-agent subcontracting and platform fees.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee configuration\n    address public feeRecipient;\n    uint256 public platformFeeBps = 250; // 2.5% = 250 basis points\n    uint256 public constant MAX_FEE_BPS = 1000; // Max 10%\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: links to parent task\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids vs direct claim\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Proposed price (can be lower than bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n        bool exists;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids; // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;        // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;           // parentTaskId => child taskIds\n\n    // Timeout: poster can refund if no claim/bid accepted after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster, uint256 refundAmount);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n    event PlatformFeeUpdated(uint256 oldFeeBps, uint256 newFeeBps);\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New address to receive platform fees\n     */\n    function setFeeRecipient(address _newRecipient) external {\n        require(msg.sender == feeRecipient, \"Only fee recipient can update\");\n        require(_newRecipient != address(0), \"Invalid address\");\n        emit FeeRecipientUpdated(feeRecipient, _newRecipient);\n        feeRecipient = _newRecipient;\n    }\n\n    /**\n     * @notice Update the platform fee (restricted to fee recipient)\n     * @param _newFeeBps New fee in basis points (100 = 1%)\n     */\n    function setPlatformFee(uint256 _newFeeBps) external {\n        require(msg.sender == feeRecipient, \"Only fee recipient can update\");\n        require(_newFeeBps <= MAX_FEE_BPS, \"Fee too high\");\n        emit PlatformFeeUpdated(platformFeeBps, _newFeeBps);\n        platformFeeBps = _newFeeBps;\n    }\n\n    // --- Task Creation ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptingBids If true, task accepts bids; if false, direct claim allowed\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptingBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @dev Only the worker of the parent task can create subtasks\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param subtaskId Unique identifier for the subtask\n     * @param bountyAmount USDC amount for the subtask\n     * @param acceptingBids If true, subtask accepts bids\n     */\n    function createSubtask(\n        bytes32 parentTaskId,\n        bytes32 subtaskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n        require(\n            parentTask.status == Status.Claimed || parentTask.status == Status.Submitted,\n            \"Parent task must be active\"\n        );\n        require(msg.sender == parentTask.worker, \"Only parent task worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from subtask creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptingBids, parentTaskId);\n    }\n\n    // --- Bidding System ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Proposed price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Price cannot exceed bounty\");\n        require(!bids[taskId][msg.sender].exists, \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false,\n            exists: true\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to the bidder\n     * @param taskId Task to assign\n     * @param bidder Address of the winning bidder\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bids[taskId][bidder].exists, \"Bid does not exist\");\n\n        Bid storage winningBid = bids[taskId][bidder];\n        winningBid.accepted = true;\n\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (winningBid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - winningBid.price;\n            task.bounty = winningBid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, winningBid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    // --- Direct Claim (for non-bidding tasks) ---\n\n    /**\n     * @notice Claim an open task as a worker (only for non-bidding tasks)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    // --- Work Submission & Approval ---\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee\n        uint256 fee = (task.bounty * platformFeeBps) / BPS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    // --- Disputes & Refunds ---\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task and get full refund (only before any claims/bids accepted)\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster, task.bounty);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get a bid for a specific task and bidder\n     * @param taskId Task to query\n     * @param bidder Bidder address\n     */\n    function getBid(bytes32 taskId, address bidder) external view returns (Bid memory) {\n        return bids[taskId][bidder];\n    }\n\n    /**\n     * @notice Get all bidders for a task\n     * @param taskId Task to query\n     */\n    function getTaskBidders(bytes32 taskId) external view returns (address[] memory) {\n        return taskBidders[taskId];\n    }\n\n    /**\n     * @notice Get all subtask IDs for a parent task\n     * @param parentTaskId Parent task to query\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Calculate the platform fee and worker payout for a given bounty\n     * @param bounty The task bounty amount\n     */\n    function calculateFee(uint256 bounty) external view returns (uint256 fee, uint256 workerPayout) {\n        fee = (bounty * platformFeeBps) / BPS_DENOMINATOR;\n        workerPayout = bounty - fee;\n    }\n\n    /**\n     * @notice Get the current platform fee in basis points\n     */\n    function getPlatformFeeBps() external view returns (uint256) {\n        return platformFeeBps;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_01EoziDYc85MkbWVywchqoSq","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice The Agent Economy Protocol - Escrow for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, other agents bid on work,\n *      poster accepts best bid, worker delivers, payment flows automatically.\n *      Supports agent-to-agent subcontracting and platform fees.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee configuration\n    address public feeRecipient;\n    uint256 public platformFeeBps = 250; // 2.5% = 250 basis points\n    uint256 public constant MAX_FEE_BPS = 1000; // Max 10%\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: links to parent task\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids vs direct claim\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Proposed price (can be lower than bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n        bool exists;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids; // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;        // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;           // parentTaskId => child taskIds\n\n    // Timeout: poster can refund if no claim/bid accepted after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster, uint256 refundAmount);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n    event PlatformFeeUpdated(uint256 oldFeeBps, uint256 newFeeBps);\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New address to receive platform fees\n     */\n    function setFeeRecipient(address _newRecipient) external {\n        require(msg.sender == feeRecipient, \"Only fee recipient can update\");\n        require(_newRecipient != address(0), \"Invalid address\");\n        emit FeeRecipientUpdated(feeRecipient, _newRecipient);\n        feeRecipient = _newRecipient;\n    }\n\n    /**\n     * @notice Update the platform fee (restricted to fee recipient)\n     * @param _newFeeBps New fee in basis points (100 = 1%)\n     */\n    function setPlatformFee(uint256 _newFeeBps) external {\n        require(msg.sender == feeRecipient, \"Only fee recipient can update\");\n        require(_newFeeBps <= MAX_FEE_BPS, \"Fee too high\");\n        emit PlatformFeeUpdated(platformFeeBps, _newFeeBps);\n        platformFeeBps = _newFeeBps;\n    }\n\n    // --- Task Creation ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptingBids If true, task accepts bids; if false, direct claim allowed\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptingBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @dev Only the worker of the parent task can create subtasks\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param subtaskId Unique identifier for the subtask\n     * @param bountyAmount USDC amount for the subtask\n     * @param acceptingBids If true, subtask accepts bids\n     */\n    function createSubtask(\n        bytes32 parentTaskId,\n        bytes32 subtaskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n        require(\n            parentTask.status == Status.Claimed || parentTask.status == Status.Submitted,\n            \"Parent task must be active\"\n        );\n        require(msg.sender == parentTask.worker, \"Only parent task worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from subtask creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptingBids, parentTaskId);\n    }\n\n    // --- Bidding System ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Proposed price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Price cannot exceed bounty\");\n        require(!bids[taskId][msg.sender].exists, \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false,\n            exists: true\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to the bidder\n     * @param taskId Task to assign\n     * @param bidder Address of the winning bidder\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bids[taskId][bidder].exists, \"Bid does not exist\");\n\n        Bid storage winningBid = bids[taskId][bidder];\n        winningBid.accepted = true;\n\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (winningBid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - winningBid.price;\n            task.bounty = winningBid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, winningBid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    // --- Direct Claim (for non-bidding tasks) ---\n\n    /**\n     * @notice Claim an open task as a worker (only for non-bidding tasks)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    // --- Work Submission & Approval ---\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee\n        uint256 fee = (task.bounty * platformFeeBps) / BPS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    // --- Disputes & Refunds ---\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task and get full refund (only before any claims/bids accepted)\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster, task.bounty);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get a bid for a specific task and bidder\n     * @param taskId Task to query\n     * @param bidder Bidder address\n     */\n    function getBid(bytes32 taskId, address bidder) external view returns (Bid memory) {\n        return bids[taskId][bidder];\n    }\n\n    /**\n     * @notice Get all bidders for a task\n     * @param taskId Task to query\n     */\n    function getTaskBidders(bytes32 taskId) external view returns (address[] memory) {\n        return taskBidders[taskId];\n    }\n\n    /**\n     * @notice Get all subtask IDs for a parent task\n     * @param parentTaskId Parent task to query\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Calculate the platform fee and worker payout for a given bounty\n     * @param bounty The task bounty amount\n     */\n    function calculateFee(uint256 bounty) external view returns (uint256 fee, uint256 workerPayout) {\n        fee = (bounty * platformFeeBps) / BPS_DENOMINATOR;\n        workerPayout = bounty - fee;\n    }\n\n    /**\n     * @notice Get the current platform fee in basis points\n     */\n    function getPlatformFeeBps() external view returns (uint256) {\n        return platformFeeBps;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_01848jD7Cb1JL2KCKSo9C1Az","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice The Agent Economy Protocol - Escrow for USDC-based agent task marketplace\n * @dev Agents post tasks with USDC bounties, other agents bid on work,\n *      poster accepts best bid, worker delivers, payment flows automatically.\n *      Supports agent-to-agent subcontracting and platform fees.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee configuration\n    address public feeRecipient;\n    uint256 public platformFeeBps = 250; // 2.5% = 250 basis points\n    uint256 public constant MAX_FEE_BPS = 1000; // Max 10%\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;    // For subtasks: links to parent task\n        uint256 bidCount;        // Number of bids received\n        bool acceptingBids;      // Whether task is accepting bids vs direct claim\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;           // Proposed price (can be lower than bounty)\n        uint256 estimatedTime;   // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n        bool exists;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(address => Bid)) public bids; // taskId => bidder => Bid\n    mapping(bytes32 => address[]) public taskBidders;        // taskId => list of bidder addresses\n    mapping(bytes32 => bytes32[]) public subtasks;           // parentTaskId => child taskIds\n\n    // Timeout: poster can refund if no claim/bid accepted after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(\n        bytes32 indexed taskId,\n        address indexed poster,\n        uint256 bounty,\n        bool acceptingBids,\n        bytes32 parentTaskId\n    );\n    event TaskBid(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price,\n        uint256 estimatedTime\n    );\n    event BidAccepted(\n        bytes32 indexed taskId,\n        address indexed bidder,\n        uint256 price\n    );\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(\n        bytes32 indexed taskId,\n        address indexed worker,\n        uint256 workerPayout,\n        uint256 platformFee\n    );\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskCancelled(bytes32 indexed taskId, address indexed poster, uint256 refundAmount);\n    event SubtaskCreated(\n        bytes32 indexed parentTaskId,\n        bytes32 indexed subtaskId,\n        address indexed creator,\n        uint256 bounty\n    );\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n    event PlatformFeeUpdated(uint256 oldFeeBps, uint256 newFeeBps);\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n    }\n\n    // --- Admin Functions ---\n\n    /**\n     * @notice Update the fee recipient address\n     * @param _newRecipient New address to receive platform fees\n     */\n    function setFeeRecipient(address _newRecipient) external {\n        require(msg.sender == feeRecipient, \"Only fee recipient can update\");\n        require(_newRecipient != address(0), \"Invalid address\");\n        emit FeeRecipientUpdated(feeRecipient, _newRecipient);\n        feeRecipient = _newRecipient;\n    }\n\n    /**\n     * @notice Update the platform fee (restricted to fee recipient)\n     * @param _newFeeBps New fee in basis points (100 = 1%)\n     */\n    function setPlatformFee(uint256 _newFeeBps) external {\n        require(msg.sender == feeRecipient, \"Only fee recipient can update\");\n        require(_newFeeBps <= MAX_FEE_BPS, \"Fee too high\");\n        emit PlatformFeeUpdated(platformFeeBps, _newFeeBps);\n        platformFeeBps = _newFeeBps;\n    }\n\n    // --- Task Creation ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     * @param acceptingBids If true, task accepts bids; if false, direct claim allowed\n     */\n    function createTask(\n        bytes32 taskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from poster to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        emit TaskCreated(taskId, msg.sender, bountyAmount, acceptingBids, bytes32(0));\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @dev Only the worker of the parent task can create subtasks\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param subtaskId Unique identifier for the subtask\n     * @param bountyAmount USDC amount for the subtask\n     * @param acceptingBids If true, subtask accepts bids\n     */\n    function createSubtask(\n        bytes32 parentTaskId,\n        bytes32 subtaskId,\n        uint256 bountyAmount,\n        bool acceptingBids\n    ) external nonReentrant {\n        Task storage parentTask = tasks[parentTaskId];\n        require(parentTask.poster != address(0), \"Parent task does not exist\");\n        require(\n            parentTask.status == Status.Claimed || parentTask.status == Status.Submitted,\n            \"Parent task must be active\"\n        );\n        require(msg.sender == parentTask.worker, \"Only parent task worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        // Transfer USDC from subtask creator to this contract\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0,\n            acceptingBids: acceptingBids\n        });\n\n        subtasks[parentTaskId].push(subtaskId);\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount, acceptingBids, parentTaskId);\n    }\n\n    // --- Bidding System ---\n\n    /**\n     * @notice Place a bid on a task\n     * @param taskId Task to bid on\n     * @param price Proposed price (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(task.acceptingBids, \"Task not accepting bids\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Price cannot exceed bounty\");\n        require(!bids[taskId][msg.sender].exists, \"Already bid on this task\");\n\n        bids[taskId][msg.sender] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false,\n            exists: true\n        });\n\n        taskBidders[taskId].push(msg.sender);\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to the bidder\n     * @param taskId Task to assign\n     * @param bidder Address of the winning bidder\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bids[taskId][bidder].exists, \"Bid does not exist\");\n\n        Bid storage winningBid = bids[taskId][bidder];\n        winningBid.accepted = true;\n\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price is less than bounty, refund the difference to poster\n        if (winningBid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - winningBid.price;\n            task.bounty = winningBid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bidder, winningBid.price);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    // --- Direct Claim (for non-bidding tasks) ---\n\n    /**\n     * @notice Claim an open task as a worker (only for non-bidding tasks)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(!task.acceptingBids, \"Task requires bidding\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    // --- Work Submission & Approval ---\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable (e.g., IPFS CID hash)\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC to worker (minus platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee\n        uint256 fee = (task.bounty * platformFeeBps) / BPS_DENOMINATOR;\n        uint256 workerPayout = task.bounty - fee;\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n        }\n\n        // Release remaining USDC to worker\n        usdc.safeTransfer(task.worker, workerPayout);\n\n        emit TaskApproved(taskId, task.worker, workerPayout, fee);\n    }\n\n    // --- Disputes & Refunds ---\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task and get full refund (only before any claims/bids accepted)\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Can only cancel open tasks\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId, task.poster, task.bounty);\n    }\n\n    // --- View Functions ---\n\n    /**\n     * @notice Get task details\n     * @param taskId Task to query\n     */\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    /**\n     * @notice Get a bid for a specific task and bidder\n     * @param taskId Task to query\n     * @param bidder Bidder address\n     */\n    function getBid(bytes32 taskId, address bidder) external view returns (Bid memory) {\n        return bids[taskId][bidder];\n    }\n\n    /**\n     * @notice Get all bidders for a task\n     * @param taskId Task to query\n     */\n    function getTaskBidders(bytes32 taskId) external view returns (address[] memory) {\n        return taskBidders[taskId];\n    }\n\n    /**\n     * @notice Get all subtask IDs for a parent task\n     * @param parentTaskId Parent task to query\n     */\n    function getSubtasks(bytes32 parentTaskId) external view returns (bytes32[] memory) {\n        return subtasks[parentTaskId];\n    }\n\n    /**\n     * @notice Calculate the platform fee and worker payout for a given bounty\n     * @param bounty The task bounty amount\n     */\n    function calculateFee(uint256 bounty) external view returns (uint256 fee, uint256 workerPayout) {\n        fee = (bounty * platformFeeBps) / BPS_DENOMINATOR;\n        workerPayout = bounty - fee;\n    }\n\n    /**\n     * @notice Get the current platform fee in basis points\n     */\n    function getPlatformFeeBps() external view returns (uint256) {\n        return platformFeeBps;\n    }\n}\n"}}],"uuid":"6b62f8b0-89aa-4fea-b348-bd8c64941577"}
