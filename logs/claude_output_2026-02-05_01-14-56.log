and platform fees\n * @dev Agents post tasks with USDC bounties. Other agents bid on tasks.\n *      Poster accepts a bid, worker delivers, poster approves, USDC flows.\n *      2.5% platform fee on every completed task.\n *      Agents can create subtasks (agent-to-agent subcontracting).\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n    address public feeRecipient;\n    uint256 public constant FEE_BPS = 250; // 2.5% = 250 basis points\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;  // bytes32(0) if top-level task\n        uint256 bidCount;\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;\n        uint256 estimatedTime; // seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(uint256 => Bid)) public bids; // taskId => index => Bid\n    mapping(bytes32 => bytes32[]) public subtaskIds; // parentTaskId => child taskIds\n\n    // Platform stats\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalFeesCollected;\n    uint256 public totalVolumeUsdc;\n\n    // Timeouts\n    uint256 public claimTimeout = 7 days;\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskBid(bytes32 indexed taskId, address indexed bidder, uint256 price, uint256 estimatedTime);\n    event BidAccepted(bytes32 indexed taskId, address indexed bidder, uint256 price);\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(bytes32 indexed taskId, address indexed worker, uint256 payout, uint256 fee);\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 amount);\n    event TaskCancelled(bytes32 indexed taskId);\n    event SubtaskCreated(bytes32 indexed parentTaskId, bytes32 indexed subtaskId, address indexed poster, uint256 bounty);\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Only owner\");\n        _;\n    }\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n        owner = msg.sender;\n    }\n\n    // â”€â”€â”€ Task Creation â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (6 decimals)\n     */\n    function createTask(bytes32 taskId, uint256 bountyAmount) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0\n        });\n\n        totalTasksCreated++;\n        emit TaskCreated(taskId, msg.sender, bountyAmount);\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param bountyAmount USDC bounty for the subtask\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount\n    ) external nonReentrant {\n        Task storage parent = tasks[parentTaskId];\n        require(parent.poster != address(0), \"Parent task does not exist\");\n        require(parent.status == Status.Claimed, \"Parent must be claimed\");\n        require(msg.sender == parent.worker, \"Only parent worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0\n        });\n\n        subtaskIds[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount);\n    }\n\n    // â”€â”€â”€ Bidding System â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Place a bid on an open task\n     * @param taskId Task to bid on\n     * @param price Proposed USDC price (can differ from bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid cannot exceed bounty\");\n\n        uint256 bidIndex = task.bidCount;\n        bids[taskId][bidIndex] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to that bidder\n     * @param taskId Task to assign\n     * @param bidIndex Index of the bid to accept\n     */\n    function acceptBid(bytes32 taskId, uint256 bidIndex) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bidIndex < task.bidCount, \"Invalid bid index\");\n\n        Bid storage bid = bids[taskId][bidIndex];\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bid.bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price < bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bid.bidder, bid.price);\n        emit TaskClaimed(taskId, bid.bidder);\n    }\n\n    // â”€â”€â”€ Task Lifecycle (backward compatible) â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Claim an open task directly (no bidding)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC (minus 2.5% platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * FEE_BPS) / BPS_DENOMINATOR;\n        uint256 payout = task.bounty - fee;\n\n        // Transfer payout to worker, fee to platform\n        usdc.safeTransfer(task.worker, payout);\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n        }\n\n        totalTasksCompleted++;\n        totalFeesCollected += fee;\n        totalVolumeUsdc += task.bounty;\n\n        emit TaskApproved(taskId, task.worker, payout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if unclaimed after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task with no bids (poster gets full refund)\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Task not open\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId);\n    }\n\n    // â”€â”€â”€ View Functions â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    function getBid(bytes32 taskId, uint256 bidIndex) external view returns (Bid memory) {\n        require(bidIndex < tasks[taskId].bidCount, \"Invalid bid index\");\n        return bids[taskId][bidIndex];\n    }\n\n    function getBidCount(bytes32 taskId) external view returns (uint256) {\n        return tasks[taskId].bidCount;\n    }\n\n    function getSubtaskIds(bytes32 taskId) external view returns (bytes32[] memory) {\n        return subtaskIds[taskId];\n    }\n\n    function getSubtaskCount(bytes32 taskId) external view returns (uint256) {\n        return subtaskIds[taskId].length;\n    }\n\n    function getPlatformStats() external view returns (\n        uint256 _totalTasksCreated,\n        uint256 _totalTasksCompleted,\n        uint256 _totalFeesCollected,\n        uint256 _totalVolumeUsdc\n    ) {\n        return (totalTasksCreated, totalTasksCompleted, totalFeesCollected, totalVolumeUsdc);\n    }\n\n    // â”€â”€â”€ Admin â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    function setFeeRecipient(address _newFeeRecipient) external onlyOwner {\n        require(_newFeeRecipient != address(0), \"Invalid fee recipient\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newFeeRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newFeeRecipient);\n    }\n\n    function transferOwnership(address newOwner) external onlyOwner {\n        require(newOwner != address(0), \"Invalid owner\");\n        owner = newOwner;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_01WeaSE7yY3CgYbWeUaGFSot","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Agent Economy Protocol â€” USDC escrow with bidding, subtasks, and platform fees\n * @dev Agents post tasks with USDC bounties. Other agents bid on tasks.\n *      Poster accepts a bid, worker delivers, poster approves, USDC flows.\n *      2.5% platform fee on every completed task.\n *      Agents can create subtasks (agent-to-agent subcontracting).\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n    address public feeRecipient;\n    uint256 public constant FEE_BPS = 250; // 2.5% = 250 basis points\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;  // bytes32(0) if top-level task\n        uint256 bidCount;\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;\n        uint256 estimatedTime; // seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(uint256 => Bid)) public bids; // taskId => index => Bid\n    mapping(bytes32 => bytes32[]) public subtaskIds; // parentTaskId => child taskIds\n\n    // Platform stats\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalFeesCollected;\n    uint256 public totalVolumeUsdc;\n\n    // Timeouts\n    uint256 public claimTimeout = 7 days;\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskBid(bytes32 indexed taskId, address indexed bidder, uint256 price, uint256 estimatedTime);\n    event BidAccepted(bytes32 indexed taskId, address indexed bidder, uint256 price);\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(bytes32 indexed taskId, address indexed worker, uint256 payout, uint256 fee);\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 amount);\n    event TaskCancelled(bytes32 indexed taskId);\n    event SubtaskCreated(bytes32 indexed parentTaskId, bytes32 indexed subtaskId, address indexed poster, uint256 bounty);\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Only owner\");\n        _;\n    }\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n        owner = msg.sender;\n    }\n\n    // â”€â”€â”€ Task Creation â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (6 decimals)\n     */\n    function createTask(bytes32 taskId, uint256 bountyAmount) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0\n        });\n\n        totalTasksCreated++;\n        emit TaskCreated(taskId, msg.sender, bountyAmount);\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param bountyAmount USDC bounty for the subtask\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount\n    ) external nonReentrant {\n        Task storage parent = tasks[parentTaskId];\n        require(parent.poster != address(0), \"Parent task does not exist\");\n        require(parent.status == Status.Claimed, \"Parent must be claimed\");\n        require(msg.sender == parent.worker, \"Only parent worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0\n        });\n\n        subtaskIds[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount);\n    }\n\n    // â”€â”€â”€ Bidding System â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Place a bid on an open task\n     * @param taskId Task to bid on\n     * @param price Proposed USDC price (can differ from bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid cannot exceed bounty\");\n\n        uint256 bidIndex = task.bidCount;\n        bids[taskId][bidIndex] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to that bidder\n     * @param taskId Task to assign\n     * @param bidIndex Index of the bid to accept\n     */\n    function acceptBid(bytes32 taskId, uint256 bidIndex) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bidIndex < task.bidCount, \"Invalid bid index\");\n\n        Bid storage bid = bids[taskId][bidIndex];\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bid.bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price < bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bid.bidder, bid.price);\n        emit TaskClaimed(taskId, bid.bidder);\n    }\n\n    // â”€â”€â”€ Task Lifecycle (backward compatible) â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Claim an open task directly (no bidding)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC (minus 2.5% platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * FEE_BPS) / BPS_DENOMINATOR;\n        uint256 payout = task.bounty - fee;\n\n        // Transfer payout to worker, fee to platform\n        usdc.safeTransfer(task.worker, payout);\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n        }\n\n        totalTasksCompleted++;\n        totalFeesCollected += fee;\n        totalVolumeUsdc += task.bounty;\n\n        emit TaskApproved(taskId, task.worker, payout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if unclaimed after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task with no bids (poster gets full refund)\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Task not open\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId);\n    }\n\n    // â”€â”€â”€ View Functions â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    function getBid(bytes32 taskId, uint256 bidIndex) external view returns (Bid memory) {\n        require(bidIndex < tasks[taskId].bidCount, \"Invalid bid index\");\n        return bids[taskId][bidIndex];\n    }\n\n    function getBidCount(bytes32 taskId) external view returns (uint256) {\n        return tasks[taskId].bidCount;\n    }\n\n    function getSubtaskIds(bytes32 taskId) external view returns (bytes32[] memory) {\n        return subtaskIds[taskId];\n    }\n\n    function getSubtaskCount(bytes32 taskId) external view returns (uint256) {\n        return subtaskIds[taskId].length;\n    }\n\n    function getPlatformStats() external view returns (\n        uint256 _totalTasksCreated,\n        uint256 _totalTasksCompleted,\n        uint256 _totalFeesCollected,\n        uint256 _totalVolumeUsdc\n    ) {\n        return (totalTasksCreated, totalTasksCompleted, totalFeesCollected, totalVolumeUsdc);\n    }\n\n    // â”€â”€â”€ Admin â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    function setFeeRecipient(address _newFeeRecipient) external onlyOwner {\n        require(_newFeeRecipient != address(0), \"Invalid fee recipient\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newFeeRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newFeeRecipient);\n    }\n\n    function transferOwnership(address newOwner) external onlyOwner {\n        require(newOwner != address(0), \"Invalid owner\");\n        owner = newOwner;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_014JgYZGVmskW1JAafDzwGY6","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Agent Economy Protocol â€” USDC escrow with bidding, subtasks, and platform fees\n * @dev Agents post tasks with USDC bounties. Other agents bid on tasks.\n *      Poster accepts a bid, worker delivers, poster approves, USDC flows.\n *      2.5% platform fee on every completed task.\n *      Agents can create subtasks (agent-to-agent subcontracting).\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n    address public feeRecipient;\n    uint256 public constant FEE_BPS = 250; // 2.5% = 250 basis points\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded, Cancelled }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;  // bytes32(0) if top-level task\n        uint256 bidCount;\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;\n        uint256 estimatedTime; // seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => mapping(uint256 => Bid)) public bids; // taskId => index => Bid\n    mapping(bytes32 => bytes32[]) public subtaskIds; // parentTaskId => child taskIds\n\n    // Platform stats\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n    uint256 public totalFeesCollected;\n    uint256 public totalVolumeUsdc;\n\n    // Timeouts\n    uint256 public claimTimeout = 7 days;\n    uint256 public disputeWindow = 3 days;\n\n    // --- Events ---\n    event TaskCreated(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskBid(bytes32 indexed taskId, address indexed bidder, uint256 price, uint256 estimatedTime);\n    event BidAccepted(bytes32 indexed taskId, address indexed bidder, uint256 price);\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(bytes32 indexed taskId, address indexed worker, uint256 payout, uint256 fee);\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 amount);\n    event TaskCancelled(bytes32 indexed taskId);\n    event SubtaskCreated(bytes32 indexed parentTaskId, bytes32 indexed subtaskId, address indexed poster, uint256 bounty);\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Only owner\");\n        _;\n    }\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n        owner = msg.sender;\n    }\n\n    // â”€â”€â”€ Task Creation â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (6 decimals)\n     */\n    function createTask(bytes32 taskId, uint256 bountyAmount) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0\n        });\n\n        totalTasksCreated++;\n        emit TaskCreated(taskId, msg.sender, bountyAmount);\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent-to-agent subcontracting)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param bountyAmount USDC bounty for the subtask\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount\n    ) external nonReentrant {\n        Task storage parent = tasks[parentTaskId];\n        require(parent.poster != address(0), \"Parent task does not exist\");\n        require(parent.status == Status.Claimed, \"Parent must be claimed\");\n        require(msg.sender == parent.worker, \"Only parent worker can create subtasks\");\n        require(tasks[subtaskId].poster == address(0), \"Subtask ID already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0\n        });\n\n        subtaskIds[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount);\n    }\n\n    // â”€â”€â”€ Bidding System â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Place a bid on an open task\n     * @param taskId Task to bid on\n     * @param price Proposed USDC price (can differ from bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid cannot exceed bounty\");\n\n        uint256 bidIndex = task.bidCount;\n        bids[taskId][bidIndex] = Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        });\n        task.bidCount++;\n\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid and assign the task to that bidder\n     * @param taskId Task to assign\n     * @param bidIndex Index of the bid to accept\n     */\n    function acceptBid(bytes32 taskId, uint256 bidIndex) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bids\");\n        require(bidIndex < task.bidCount, \"Invalid bid index\");\n\n        Bid storage bid = bids[taskId][bidIndex];\n        require(!bid.accepted, \"Bid already accepted\");\n\n        bid.accepted = true;\n        task.worker = bid.bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        // If bid price < bounty, refund the difference to poster\n        if (bid.price < task.bounty) {\n            uint256 refundAmount = task.bounty - bid.price;\n            task.bounty = bid.price;\n            usdc.safeTransfer(task.poster, refundAmount);\n        }\n\n        emit BidAccepted(taskId, bid.bidder, bid.price);\n        emit TaskClaimed(taskId, bid.bidder);\n    }\n\n    // â”€â”€â”€ Task Lifecycle (backward compatible) â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    /**\n     * @notice Claim an open task directly (no bidding)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC (minus 2.5% platform fee)\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * FEE_BPS) / BPS_DENOMINATOR;\n        uint256 payout = task.bounty - fee;\n\n        // Transfer payout to worker, fee to platform\n        usdc.safeTransfer(task.worker, payout);\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n        }\n\n        totalTasksCompleted++;\n        totalFeesCollected += fee;\n        totalVolumeUsdc += task.bounty;\n\n        emit TaskApproved(taskId, task.worker, payout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if unclaimed after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        // Refund if open and claim timeout expired\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        // Refund if disputed\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    /**\n     * @notice Cancel an open task (poster gets full refund)\n     * @param taskId Task to cancel\n     */\n    function cancelTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can cancel\");\n        require(task.status == Status.Open, \"Task not open\");\n\n        task.status = Status.Cancelled;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskCancelled(taskId);\n    }\n\n    // â”€â”€â”€ View Functions â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    function getBid(bytes32 taskId, uint256 bidIndex) external view returns (Bid memory) {\n        require(bidIndex < tasks[taskId].bidCount, \"Invalid bid index\");\n        return bids[taskId][bidIndex];\n    }\n\n    function getBidCount(bytes32 taskId) external view returns (uint256) {\n        return tasks[taskId].bidCount;\n    }\n\n    function getSubtaskIds(bytes32 taskId) external view returns (bytes32[] memory) {\n        return subtaskIds[taskId];\n    }\n\n    function getSubtaskCount(bytes32 taskId) external view returns (uint256) {\n        return subtaskIds[taskId].length;\n    }\n\n    function getPlatformStats() external view returns (\n        uint256 _totalTasksCreated,\n        uint256 _totalTasksCompleted,\n        uint256 _totalFeesCollected,\n        uint256 _totalVolumeUsdc\n    ) {\n        return (totalTasksCreated, totalTasksCompleted, totalFeesCollected, totalVolumeUsdc);\n    }\n\n    // â”€â”€â”€ Admin â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n\n    function setFeeRecipient(address _newFeeRecipient) external onlyOwner {\n        require(_newFeeRecipient != address(0), \"Invalid fee recipient\");\n        address oldRecipient = feeRecipient;\n        feeRecipient = _newFeeRecipient;\n        emit FeeRecipientUpdated(oldRecipient, _newFeeRecipient);\n    }\n\n    function transferOwnership(address newOwner) external onlyOwner {\n        require(newOwner != address(0), \"Invalid owner\");\n        owner = newOwner;\n    }\n}\n"}}],"uuid":"c676dd71-8144-4439-a498-342c1bb31ec3"}
