gents post tasks with USDC bounties, other agents bid, poster accepts best bid,\n *      worker delivers, poster approves, USDC flows minus platform fee.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% (250 basis points)\n    uint256 public constant PLATFORM_FEE_BPS = 250;\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    address public feeRecipient;\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, Approved, Disputed, Refunded }\n\n    struct Task {\n        address poster;\n        address worker;\n        uint256 bounty;\n        Status status;\n        bytes32 deliverableHash;\n        uint256 createdAt;\n        uint256 claimedAt;\n        uint256 submittedAt;\n        bytes32 parentTaskId;   // 0x0 if no parent (top-level task)\n        uint256 bidCount;\n    }\n\n    struct Bid {\n        address bidder;\n        uint256 price;          // USDC amount the bidder will accept\n        uint256 estimatedTime;  // Estimated delivery time in seconds\n        uint256 timestamp;\n        bool accepted;\n    }\n\n    mapping(bytes32 => Task) public tasks;\n    mapping(bytes32 => Bid[]) public taskBids;\n    mapping(bytes32 => bytes32[]) public subtaskIds; // parentTaskId => child task IDs\n\n    // Timeout: poster can refund if no claim after this period\n    uint256 public claimTimeout = 7 days;\n    // Dispute window: poster can dispute within this period after submission\n    uint256 public disputeWindow = 3 days;\n\n    // Platform stats\n    uint256 public totalFeesCollected;\n    uint256 public totalTasksCreated;\n    uint256 public totalTasksCompleted;\n\n    // --- Events ---\n    event TaskCreated(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event TaskBid(bytes32 indexed taskId, address indexed bidder, uint256 price, uint256 estimatedTime);\n    event BidAccepted(bytes32 indexed taskId, address indexed bidder, uint256 price);\n    event TaskClaimed(bytes32 indexed taskId, address indexed worker);\n    event DeliverableSubmitted(bytes32 indexed taskId, bytes32 deliverableHash);\n    event TaskApproved(bytes32 indexed taskId, address indexed worker, uint256 payout, uint256 fee);\n    event TaskDisputed(bytes32 indexed taskId);\n    event TaskRefunded(bytes32 indexed taskId, address indexed poster, uint256 bounty);\n    event SubtaskCreated(bytes32 indexed parentTaskId, bytes32 indexed subtaskId, address indexed poster, uint256 bounty);\n    event FeeRecipientUpdated(address indexed oldRecipient, address indexed newRecipient);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Only owner\");\n        _;\n    }\n\n    constructor(address _usdc, address _feeRecipient) {\n        require(_usdc != address(0), \"Invalid USDC address\");\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        usdc = IERC20(_usdc);\n        feeRecipient = _feeRecipient;\n        owner = msg.sender;\n    }\n\n    // --- Admin ---\n\n    function setFeeRecipient(address _feeRecipient) external onlyOwner {\n        require(_feeRecipient != address(0), \"Invalid fee recipient\");\n        address old = feeRecipient;\n        feeRecipient = _feeRecipient;\n        emit FeeRecipientUpdated(old, _feeRecipient);\n    }\n\n    // --- Task Lifecycle ---\n\n    /**\n     * @notice Post a new task with USDC bounty (deposited into escrow)\n     * @param taskId Unique task identifier\n     * @param bountyAmount USDC amount (in smallest unit, 6 decimals)\n     */\n    function createTask(bytes32 taskId, uint256 bountyAmount) external nonReentrant {\n        require(tasks[taskId].poster == address(0), \"Task already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[taskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: bytes32(0),\n            bidCount: 0\n        });\n\n        totalTasksCreated++;\n        emit TaskCreated(taskId, msg.sender, bountyAmount);\n    }\n\n    /**\n     * @notice Create a subtask linked to a parent task (agent supply chain)\n     * @param subtaskId Unique subtask identifier\n     * @param parentTaskId The parent task this subtask belongs to\n     * @param bountyAmount USDC bounty for the subtask\n     */\n    function createSubtask(\n        bytes32 subtaskId,\n        bytes32 parentTaskId,\n        uint256 bountyAmount\n    ) external nonReentrant {\n        Task storage parent = tasks[parentTaskId];\n        require(parent.poster != address(0), \"Parent task does not exist\");\n        require(\n            msg.sender == parent.worker || msg.sender == parent.poster,\n            \"Only parent worker or poster can create subtask\"\n        );\n        require(tasks[subtaskId].poster == address(0), \"Subtask already exists\");\n        require(bountyAmount > 0, \"Bounty must be > 0\");\n\n        usdc.safeTransferFrom(msg.sender, address(this), bountyAmount);\n\n        tasks[subtaskId] = Task({\n            poster: msg.sender,\n            worker: address(0),\n            bounty: bountyAmount,\n            status: Status.Open,\n            deliverableHash: bytes32(0),\n            createdAt: block.timestamp,\n            claimedAt: 0,\n            submittedAt: 0,\n            parentTaskId: parentTaskId,\n            bidCount: 0\n        });\n\n        subtaskIds[parentTaskId].push(subtaskId);\n        totalTasksCreated++;\n\n        emit SubtaskCreated(parentTaskId, subtaskId, msg.sender, bountyAmount);\n        emit TaskCreated(subtaskId, msg.sender, bountyAmount);\n    }\n\n    // --- Bidding ---\n\n    /**\n     * @notice Bid on an open task\n     * @param taskId Task to bid on\n     * @param price USDC amount the bidder will accept (can be <= bounty)\n     * @param estimatedTime Estimated delivery time in seconds\n     */\n    function bidOnTask(\n        bytes32 taskId,\n        uint256 price,\n        uint256 estimatedTime\n    ) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot bid on own task\");\n        require(price > 0, \"Price must be > 0\");\n        require(price <= task.bounty, \"Bid price exceeds bounty\");\n\n        // Check that bidder hasn't already bid\n        Bid[] storage bids = taskBids[taskId];\n        for (uint256 i = 0; i < bids.length; i++) {\n            require(bids[i].bidder != msg.sender, \"Already bid on this task\");\n        }\n\n        bids.push(Bid({\n            bidder: msg.sender,\n            price: price,\n            estimatedTime: estimatedTime,\n            timestamp: block.timestamp,\n            accepted: false\n        }));\n\n        task.bidCount++;\n        emit TaskBid(taskId, msg.sender, price, estimatedTime);\n    }\n\n    /**\n     * @notice Accept a bid on a task — assigns the bidder as worker\n     * @param taskId Task to accept bid for\n     * @param bidder Address of the bidder to accept\n     */\n    function acceptBid(bytes32 taskId, address bidder) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender == task.poster, \"Only poster can accept bid\");\n\n        Bid[] storage bids = taskBids[taskId];\n        bool found = false;\n        for (uint256 i = 0; i < bids.length; i++) {\n            if (bids[i].bidder == bidder) {\n                bids[i].accepted = true;\n                found = true;\n                break;\n            }\n        }\n        require(found, \"Bid not found\");\n\n        task.worker = bidder;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit BidAccepted(taskId, bidder, task.bounty);\n        emit TaskClaimed(taskId, bidder);\n    }\n\n    /**\n     * @notice Claim an open task directly (no bidding)\n     * @param taskId Task to claim\n     */\n    function claimTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.poster != address(0), \"Task does not exist\");\n        require(task.status == Status.Open, \"Task not open\");\n        require(msg.sender != task.poster, \"Poster cannot claim own task\");\n\n        task.worker = msg.sender;\n        task.status = Status.Claimed;\n        task.claimedAt = block.timestamp;\n\n        emit TaskClaimed(taskId, msg.sender);\n    }\n\n    /**\n     * @notice Submit a deliverable hash as proof of work\n     * @param taskId Task to submit for\n     * @param deliverableHash Hash of the deliverable\n     */\n    function submitDeliverable(bytes32 taskId, bytes32 deliverableHash) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Claimed, \"Task not claimed\");\n        require(msg.sender == task.worker, \"Only worker can submit\");\n        require(deliverableHash != bytes32(0), \"Invalid deliverable hash\");\n\n        task.deliverableHash = deliverableHash;\n        task.status = Status.Submitted;\n        task.submittedAt = block.timestamp;\n\n        emit DeliverableSubmitted(taskId, deliverableHash);\n    }\n\n    /**\n     * @notice Approve the deliverable and release USDC (minus platform fee) to worker\n     * @param taskId Task to approve\n     */\n    function approveTask(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can approve\");\n\n        task.status = Status.Approved;\n\n        // Calculate platform fee (2.5%)\n        uint256 fee = (task.bounty * PLATFORM_FEE_BPS) / BPS_DENOMINATOR;\n        uint256 payout = task.bounty - fee;\n\n        // Transfer payout to worker\n        usdc.safeTransfer(task.worker, payout);\n\n        // Transfer fee to platform\n        if (fee > 0) {\n            usdc.safeTransfer(feeRecipient, fee);\n            totalFeesCollected += fee;\n        }\n\n        totalTasksCompleted++;\n        emit TaskApproved(taskId, task.worker, payout, fee);\n    }\n\n    /**\n     * @notice Dispute a submitted deliverable (within dispute window)\n     * @param taskId Task to dispute\n     */\n    function disputeTask(bytes32 taskId) external {\n        Task storage task = tasks[taskId];\n        require(task.status == Status.Submitted, \"Task not submitted\");\n        require(msg.sender == task.poster, \"Only poster can dispute\");\n        require(\n            block.timestamp <= task.submittedAt + disputeWindow,\n            \"Dispute window expired\"\n        );\n\n        task.status = Status.Disputed;\n        emit TaskDisputed(taskId);\n    }\n\n    /**\n     * @notice Refund the poster (if no claim after timeout, or after dispute)\n     * @param taskId Task to refund\n     */\n    function refund(bytes32 taskId) external nonReentrant {\n        Task storage task = tasks[taskId];\n        require(msg.sender == task.poster, \"Only poster can refund\");\n\n        bool canRefund = false;\n\n        if (task.status == Status.Open && block.timestamp > task.createdAt + claimTimeout) {\n            canRefund = true;\n        }\n\n        if (task.status == Status.Disputed) {\n            canRefund = true;\n        }\n\n        require(canRefund, \"Cannot refund at this time\");\n\n        task.status = Status.Refunded;\n        usdc.safeTransfer(task.poster, task.bounty);\n\n        emit TaskRefunded(taskId, task.poster, task.bounty);\n    }\n\n    // --- View Functions ---\n\n    function getTask(bytes32 taskId) external view returns (Task memory) {\n        return tasks[taskId];\n    }\n\n    function getBids(bytes32 taskId) external view returns (Bid[] memory) {\n        return taskBids[taskId];\n    }\n\n    function getSubtaskIds(bytes32 taskId) external view returns (bytes32[] memory) {\n        return subtaskIds[taskId];\n    }\n\n    function getBidCount(bytes32 taskId) external view returns (uint256) {\n        return taskBids[taskId].length;\n    }\n\n    function calculateFee(uint256 amount) external pure returns (uint256 fee, uint256 payout) {\n        fee = (amount * PLATFORM_FEE_BPS) / BPS_DENOMINATOR;\n        payout = amount - fee;\n    }\n}\n"}},{"tool_name":"Write","tool_use_id":"toolu_0117dKkiyCACtH3Va3Ack7Se","tool_input":{"file_path":"/Users/timmethy/Developer/claw-marketplace/skill/usdc-agent-tasks/contracts/TaskEscrow.sol","content":"// SPDX-License-Identifier: MIT\npragma solidity ^0.8.20;\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\n\n/**\n * @title TaskEscrow\n * @notice Agent Economy Protocol — USDC escrow for agent-to-agent task marketplace\n * @dev Supports bidding, subtasks (supply chains), platform fees, and reputation-linked workflows.\n *      Agents post tasks with USDC bounties, other agents bid, poster accepts best bid,\n *      worker delivers, poster approves, USDC flows minus platform fee.\n */\ncontract TaskEscrow is ReentrancyGuard {\n    using SafeERC20 for IERC20;\n\n    IERC20 public immutable usdc;\n\n    // Platform fee: 2.5% (250 basis points)\n    uint256 public constant PLATFORM_FEE_BPS = 250;\n    uint256 public constant BPS_DENOMINATOR = 10000;\n\n    address public feeRecipient;\n    address public owner;\n\n    enum Status { Open, Claimed, Submitted, A